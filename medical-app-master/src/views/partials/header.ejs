<html>
    <head>
        <title>Blog App</title>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.1.4/semantic.min.css">
        <style>
            i.icon{
              font-size: 2em;
            }
            .container.main{
             margin-top: 7em;
            }
            #delete{
             display: inline;
            }
        </style>

    </head>
    <body>
        <div class="ui fixed inverted menu">
            <div class="ui container">
                <div class="header item"> <i class="code icon"></i>Medical Web-App </div>
            </div>
            <form action="/users/me/avatar" enctype="multipart/form-data" method="POST"> 
                <input type="file" name="avatar"  />
                <input type="submit"/>
              </form>
            <form action="/users/me?_method=DELETE"  method="POST">
                <input type="submit" name="Delete" value="Delete" class="item">
            </form>
            <form  method="POST" action="/users/logout">
                <input type="submit" name="Logout" value="Logout" class="item">
            </form>
            <form  method="POST" action="/users/logoutAll">
                <input type="submit" name="LogoutAll" value="LogoutAll" class="item">
            </form>
            <a  class="item" href="/medicine/new">New Task</a>
            <a  class="item" href="/users/me">My Info</a>
          </div>
        
        